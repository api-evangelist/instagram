                "operationId": "GetBasicInformationAboutAUser",
                "deprecated": 0,
                "parameters": []
            }
        },
        "\/users\/self\/feed": {
            "get": {
                "summary": "",
                "description": "See the authenticated user's feed.",
                "operationId": "SeeTheAuthenticatedUsersFeed",
                "deprecated": 0,
                "parameters": [
                    {
                        "name": "count",
                        "in": "query",
                        "description": "Count of media to return."
                    },
                    {
                        "name": "max_id",
                        "in": "query",
                        "description": "Return media earlier than this max_id.s"
                    },
                    {
                        "name": "min_id",
                        "in": "query",
                        "description": "Return media later than this min_id."
                    }
                ]
            }
        },
        "\/users\/{user-id}\/media\/recent": {
            "get": {
                "summary": "",
                "description": "",
                "operationId": "",
                "deprecated": 0,
                "parameters": [
                    {
                        "name": "count",
                        "in": "query",
                        "description": "Count of media to return."
                    },
                    {
                        "name": "max_timestamp",
                        "in": "query",
                        "description": "Return media before this UNIX timestamp."
